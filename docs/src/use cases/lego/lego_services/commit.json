{
  "id": "lego-commit-service",
  "type": "XYZ",
  "configuration": {
    "scope": "restricted",
    "tile_store": true,
    "referenced_service": "lego-base-service",
    "tile_size": 256,
    "minzoom": 7,
    "maxzoom": 7,
    "inject_user": true
  },
  "title": "Lego Brick Tile Commit Service",
  "description": "Service for committing a completed tile to the final map",
  "enabled": true,
  "process": {
    "parameters": [],
    "process_graph": {
      "commit": {
        "process_id": "tile_assignment",
        "result": false,
        "arguments": {
          "zoom": 7,
          "x_range": [55, 80],
          "y_range": [27, 52],
          "stage": "submit",
          "store": {
            "from_parameter": "_openeo_tile_store"
          },
          "service_id": "lego-mosaic-test-1",
          "user_id": {
            "from_parameter": "_openeo_user"
          }
        }
      },
      "save": {
        "process_id": "save_result",
        "result": true,
        "arguments": {
          "data": {
            "from_node": "commit"
          },
          "format": "json"
        }
      }
    }
  }
}
